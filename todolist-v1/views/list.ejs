<%-include("header")-%>
  <body>
    <div class="box" id="heading">
      <h1><%=title%></h1>
    </div>

<div class="box">
<% for(var i=0;i< newlistitem.length;i++) {%>
          <div class="item">
            <input type="checkbox">
      <p><%=newlistitem[i].name%></p>
      </div>
    <%}%>
    <form class="item" action="/" method="post">
      <input type="text" name="listitem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value=<%=title%>> +</button>
    </form>
</div>



</body>
<%-include("footer")-%>
